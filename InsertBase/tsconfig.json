{
	"compilerOptions": {
		"target": "es2019",
		"lib": [
            "ESNext"
		],
		"module": "commonjs",
		"allowJs": true,
		"outDir": "dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"rootDir": "src",
	},
	"include": [
		"src"
	],
}